@article{ WOS:000349583700001,
Author = {Chandler, David and Richmond, Oliver},
Title = {Contesting postliberalism: governmentality or emancipation?},
Journal = {JOURNAL OF INTERNATIONAL RELATIONS AND DEVELOPMENT},
