@article{ WOS:000895202600003,
Author = {Taylor, Philip and Gringart, Eyal and Webb, Eileen O. and Carnemolla,
   Phillippa and Drake, Deirdre and Oppert, Michelle and Harvey, Robin},
Title = {Drivers and patterns of early retirement in the neoliberal university},
