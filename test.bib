@article{ WOS:000785893400001,
Author = {King, Sanna},
Title = {The push toward institutional control: Structural violence and youth in
   HawaiModified Letter Turned Commai},
Journal = {ANALYSES OF SOCIAL ISSUES AND PUBLIC POLICY},
Year = {2022},
Volume = {22},
Number = {2},
Pages = {648-668},
Month = {AUG},
Abstract = {Concerns surrounding juvenile delinquency point to neoliberal notions of
   youth responsibilization with no regard to the violence of structural
   oppressions and daily hardships, in which deviance could be a form of
   response. This article discusses the influence of and response to
   structural violence and oppression that resulted in daily hardships for
   youth, and for some youth, deviant behaviors that pushed them toward the
   criminal justice system. Drawing on in-depth interviews with 64
   participants and 532 pieces of youth writings in HawaiModified Letter
   Turned Commai, this study examines how experiences with structural
   violence and oppression pushed youth in HawaiModified Letter Turned
   Commai toward institutional control. Findings indicate the role
   racialized and gendered structures developed through colonialism had in
   the lives of youth in HawaiModified Letter Turned Commai and their
   difficult experiences with family instability and patriarchal control
   that result in youth engaging in deviant coping mechanisms. Youth
   exhibited resilience in the face of hardship and methods of control that
   speak to the lack of choices youth have in places with histories of
   colonialism.},
