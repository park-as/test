@article{ WOS:000404726100003,
Author = {Sousa, Cindy and Marshall, David J.},
Title = {Political violence and mental health: Effects of neoliberalism and the
   role of international social work practice},
Journal = {INTERNATIONAL SOCIAL WORK},
Year = {2017},
Volume = {60},
Number = {4},
Pages = {787-799},
Month = {JUL},
Abstract = {Healing from political violence is not solely an individual project, but
   a communal process involving reclaiming collective action, trust, and
   efficacy. This article uses a case study from two Palestinian refugee
   camps in the West Bank to examine how two trends central to
   neoliberalism, individualism, and the medicalization of inherently
   social and political problems, discount larger forces that affect risk
   and resilience, thereby undermining mental health recovery from
   political violence. Implications for international social work include
   utilizing ecosocial frameworks for research and practice, engaging in
   advocacy, and establishing agendas for mental health practice that
   emphasize individual and collective self-determination.},
