@article{ WOS:000515354000012,
Author = {Urquiza, Anahi and Billi, Marco},
Title = {Water markets and social-ecological resilience to water stress in the
   context of climate change: an analysis of the Limari Basin, Chile},
Journal = {ENVIRONMENT DEVELOPMENT AND SUSTAINABILITY},
