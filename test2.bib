@article{ WOS:000846675400001,
Author = {Bowles, Benjamin O. L.},
Title = {Resilience, infrastructure and the anti-social contract in neoliberal
   Britain},
Journal = {CRITIQUE OF ANTHROPOLOGY},
Year = {2022},
Volume = {42},
Number = {3, SI},
Pages = {270-285},
Month = {SEP},
Abstract = {`Resilience', a quintessentially neoliberal concept, has never been a
   politically neutral discourse, its intellectual roots situated in the
   work of Friedrich Hayek and the birth of neoliberal economics.
   Nevertheless, resilience in infrastructure is often cast as a
   technocratic, apolitical consideration. This article argues that this is
   not the case. Using data collected during fieldwork with the UK
   Government Cabinet Office during a consultation on how to make
   infrastructure `resilient by design', resilience discourse is shown to
   be a tool with which government departments, regulators and companies
   make communities increasingly responsible for the provision and
   maintenance of their own infrastructure while justifying service
   failures as inevitable. This is an under-explored discursive
   battleground in the neoliberal reframing of the social contract as
   anti-social; concerning the profit-driven logics of corporate entities
   as balanced by the rights of individual consumers, and no longer about
   the relationship between `the state' and a collective civil society.},
